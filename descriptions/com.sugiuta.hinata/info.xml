<package>
    <id>com.sugiuta.hinata</id>
    <name>Hinata</name>
    <version>0.0.2</version>
    <compatibility>
        <firmware>
            <miniOS>14.0</miniOS>
            <maxiOS>14.5</maxiOS>
            <otherVersions>unsupported</otherVersions>
            <!--
            for otherVersions, you can put either unsupported or unconfirmed
            -->
        </firmware>
    </compatibility>

      <dependencies>
		<package>mobilesubstrate, preferenceloader, ws.hbang.common, ws.hbang.alderis (>= 1.1), com.sugiuta.libkuromi, firmware (>=14.0), com.peterdev.libpddokdo</package>
	</dependencies>

    <descriptionlist>
        <description>Custom lockscreen tweak for iOS14.</description>
    </descriptionlist>
    <screenshots></screenshots>
    <changelog>
		<change>Fixed some bugs.</change>
		<change>Added 12-Hour mode</change>
      <change>Added Color Option.</change>
      <change>Enabled to adjust the position of time and date.</change>
      <change>Added Locale Option.  You can change the language of the date.</change>
    </changelog>
    <links></links>
</package>